    Name = "Set Level",
    Default = "",
    TextDisappear = false,
    Callback = function(Value)
        if levelToggleEnabled then
            local level = tonumber(Value)
            if level then
                game:GetService("Players").LocalPlayer.leaderstats.Level.Value = level
            end
        end
    end    
})



