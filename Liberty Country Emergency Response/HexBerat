local options = {}
for name, _ in pairs(teleportSpots) do
    table.insert(options, name)
end

Tab:AddDropdown({
    Name = "Map Locations",
    Default = "1",
    Options = options,
    Callback = function(Value)
        local location = teleportSpots[Value]
        if location then
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(location)
        end
    end
})







