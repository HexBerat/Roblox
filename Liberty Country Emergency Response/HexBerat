    Default = false,
    Callback = function(Value)
        cameraZoomEnabled = Value
        if cameraZoomEnabled then
            while cameraZoomEnabled do
                player.CameraMaxZoomDistance = math.huge
                wait(0.5)
            end
        else
            player.CameraMaxZoomDistance = 20
        end
    end    
})










