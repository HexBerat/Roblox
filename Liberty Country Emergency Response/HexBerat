local options = {}
for name, _ in pairs(FishAreas) do
    table.insert(options, name)
end

Tab:AddDropdown({
    Name = "Fish Locations",
    Default = "1",
    Options = options,
    Callback = function(Value)
        local location = FishAreas[Value]
        if location then
            game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(location)
        end
    end
})







