function AntiAfk2()
    spawn(function()
        while AntiAfk do
            game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("afk"):FireServer(false)
            task.wait(0.01)
        end
    end)
end   

Tab:AddToggle({
    Name = "Anti-AFK (Remove tag)",
    Default = false,
    Callback = function(Value)
        AntiAfk = Value
        AntiAfk2()
    end    
})






